{
  "$schema": "https://schemas.wp.org/trunk/theme.json",
  "version": 3,
  "settings": {
    "blocks": {
      "core/site-logo": {},
      "core/query": {},
      "core/pullquote": {},
      "core/post-title": {},
      "core/post-template": {},
      "core/post-excerpt": {},
      "core/post-date": {},
      "core/paragraph": {},
      "core/navigation-link": {},
      "core/navigation": {},
      "core/media-text": {},
      "core/list-item": {},
      "core/list": {},
      "core/image": {},
      "core/heading": {},
      "core/group": {},
      "core/details": {},
      "core/cover": {},
      "core/columns": {},
      "core/buttons": {},
      "core/button": {}
    },
    "appearanceTools": true,
    "useRootPaddingAwareAlignments": true
  },
  "styles": {
    "blocks": {
      "core/site-logo": {},
      "core/query": {},
      "core/pullquote": {},
      "core/post-title": {},
      "core/post-template": {},
      "core/post-excerpt": {},
      "core/post-date": {},
      "core/paragraph": {},
      "core/navigation-link": {},
      "core/navigation": {},
      "core/media-text": {},
      "core/list-item": {},
      "core/list": {},
      "core/image": {},
      "core/heading": {},
      "core/group": {},
      "core/details": {},
      "core/cover": {},
      "core/columns": {},
      "core/buttons": {},
      "core/button": {}
    },
    "color": {
      "text": "var:preset|color|grey"
    },
    "typography": {
      "fontFamily": "var:preset|font-family|default",
      "lineHeight": "normal"
    },
    "spacing": {
      "padding": {
        "right": "2rem",
        "left": "2rem"
      }
    },
    "elements": {
      "link": {
        "color": {
          "text": "inherit"
        },
        "typography": {
          "textDecoration": "underline"
        },
        ":hover": {
          "typography": {
            "textDecoration": "none"
          }
        },
        ":active": {
          "color": {
            "text": "inherit"
          }
        },
        ":visited": {
          "color": {
            "text": "inherit"
          }
        }
      }
    }
  }
}